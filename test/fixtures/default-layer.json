  "testLayer2": {
    "label": "Cumulative Number of Children Vaccinated Not Vaccinated",
    "source": {
      "type": "geojson",
      "featureType": "Point",
      "data": {
        "type": "superset",
        "slice-id": 2745
      },
      "join": ["name", "name"],
      "minzoom": 3
    },
    "type": "circle",
    "property": "cumulative_not_vac",
    "radius-range": ["12", "15", "18", "21", "24"],
    "categories": {
      "breaks": "no",
      "limit": [0, 100, 200, 300, 1000000],
      "color": ["#525252", "#6baed6", "#4292c6", "#2171b5", "#08519c"],
      "label": ["Null", "0-100", "100-200", "200-300", "> 300"]
    },
    "geo-columns": ["longitude", "latitude"],

    "aggregate": {
      "filter": ["region", "province1", "municipality1", "name"],
      "accepted-filter-values": ["all", "all", "all", "all"],
      "filter-label": ["Region", "Province", "Municipality/City", "Facility Name"],
      "filter-type": ["vBool", "vBool", "vBool", "vBool"],
      "timeseries": {
        "field": "period",
        "type": "cumulative"
      }
    },

    "popup": {
      "header": "name",
      "body": " <p style=text-align:left;> <b>Children Not Vaccinated:</b> {{cumulative_not_vac}}<span style=float:right;>"
    },
    "location": {
      "zoom": 6.702550963685252,
      "center": {
        "lng": 123.18284332092298,
        "lat": 7.0467051095247655
      }
    },
    "visible": false,
    "credit": "<b>As of Current Date and Time</b>",
    "category": "Immunization Coverage"
  }
